PATH="/home/uos/Desktop/dpkg-1.19.7/dpkg-deb:/home/uos/Desktop/dpkg-1.19.7/dpkg-split:/home/uos/Desktop/dpkg-1.19.7/src:/home/uos/Desktop/dpkg-1.19.7/utils:$PATH"
